{
  "name": "@cicada-lang/lambda",
  "version": "0.2.3",
  "repository": "github:cicada-lang/lambda",
  "type": "module",
  "main": "./lib/index.js",
  "files": [
    "src",
    "lib"
  ],
  "bin": {
    "lambda": "bin/lambda.js"
  },
  "scripts": {
    "build": "rm -rf lib && tsc",
    "build:watch": "rm -rf lib && tsc --watch",
    "test:ts": "vitest --dir src --run",
    "test:watch": "vitest --dir src",
    "test:lambda:tests": "test-runner snapshot './bin/lambda.js' 'tests/**/*.(scm|md)' --exclude 'tests/**/*.error.(scm|md)'",
    "test:lambda:tests-error": "test-runner snapshot-error './bin/lambda.js' 'tests/**/*.error.(scm|md)'",
    "test:lambda": "npm run test:lambda:tests && npm run test:lambda:tests-error",
    "test": "npm run test:ts && npm run test:lambda",
    "format": "prettier src docs --write"
  },
  "dependencies": {
    "@cicada-lang/framework": "^0.2.0",
    "@cicada-lang/partech": "^0.2.5",
    "@cicada-lang/sexp": "^0.1.2",
    "@xieyuheng/command-line": "^0.1.2",
    "@xieyuheng/ty": "^0.2.1",
    "commonmark": "^0.30.0",
    "crypto-browserify": "^3.12.0",
    "fast-deep-equal": "^3.1.3",
    "picocolors": "^1.0.0",
    "readdirp": "^3.6.0"
  },
  "devDependencies": {
    "@types/commonmark": "^0.27.9",
    "@types/node": "^20.11.0",
    "@xieyuheng/test-runner": "^0.2.10",
    "prettier": "^3.1.1",
    "prettier-plugin-organize-imports": "^3.2.4",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vitest": "^1.1.3"
  },
  "license": "GPL-3.0-or-later"
}
