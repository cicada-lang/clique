{
  "name": "@cicada-lang/clique",
  "version": "0.2.3",
  "repository": "github:cicada-lang/clique",
  "type": "module",
  "main": "./lib/index.js",
  "files": [
    "src",
    "lib"
  ],
  "bin": {
    "clique": "bin/clique.js"
  },
  "scripts": {
    "build": "rm -rf lib && tsc",
    "build:watch": "rm -rf lib && tsc --watch",
    "test:node": "node --test",
    "test:lang:tests": "test-runner snapshot './bin/clique.js' 'docs/lang0/tests/**/*.(scm|md)' --exclude 'docs/lang0/tests/**/*.error.(scm|md)'",
    "test:lang:tests-error": "test-runner snapshot-error './bin/clique.js' 'docs/lang0/tests/**/*.error.(scm|md)'",
    "test:lang": "npm run test:lang:tests && npm run test:lang:tests-error",
    "test": "npm run test:node && npm run test:lang",
    "format": "prettier src docs --write"
  },
  "dependencies": {
    "@cicada-lang/framework": "^0.2.0",
    "@cicada-lang/sexp": "^0.1.2",
    "@xieyuheng/command-line": "^0.1.2",
    "@xieyuheng/ty": "^0.2.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.20",
    "@xieyuheng/test-runner": "^0.2.10",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "typescript": "^5.3.3"
  },
  "license": "GPL-3.0-or-later"
}
