{
  "name": "@cicada-lang/lambda",
  "version": "0.1.3",
  "repository": "github:cicada-lang/lambda",
  "main": "./lib/index.js",
  "files": [
    "lib"
  ],
  "bin": {
    "lambda": "bin/lambda.js"
  },
  "scripts": {
    "prepare": "husky install",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test:ts": "vitest --dir src --threads false --run",
    "test:watch": "vitest --dir src --threads false",
    "test:coverage": "vitest --dir src --threads false --run --coverage ",
    "test:gui": "vitest --dir src --threads false --ui",
    "test:lambda:tests": "test-runner snapshot './bin/lambda.js' 'tests/**/*.(scm|md)' --exclude 'tests/**/*.error.(scm|md)'",
    "test:lambda:tests-error": "test-runner snapshot-error './bin/lambda.js' 'tests/**/*.error.(scm|md)'",
    "test:lambda": "npm run test:lambda:tests && npm run test:lambda:tests-error",
    "test": "npm run test:ts && npm run test:lambda",
    "format": "prettier src docs --write"
  },
  "dependencies": {
    "@cicada-lang/framework": "^0.1.0",
    "@cicada-lang/partech": "^0.2.3",
    "@cicada-lang/sexp": "^0.0.36",
    "@xieyuheng/command-line": "^0.0.7",
    "@xieyuheng/ty": "^0.1.17",
    "commonmark": "^0.30.0",
    "crypto-browserify": "^3.12.0",
    "fast-deep-equal": "^3.1.3",
    "node-watch": "^0.7.3",
    "picocolors": "^1.0.0",
    "readdirp": "^3.6.0"
  },
  "devDependencies": {
    "@types/commonmark": "^0.27.5",
    "@types/node": "^18.11.9",
    "@vitest/coverage-c8": "^0.25.3",
    "@vitest/coverage-istanbul": "^0.25.3",
    "@vitest/ui": "^0.25.3",
    "@xieyuheng/test-runner": "^0.2.7",
    "husky": "^8.0.2",
    "prettier": "^2.8.0",
    "prettier-plugin-organize-imports": "^3.2.1",
    "pretty-quick": "^3.1.3",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vitest": "^0.25.3"
  },
  "license": "GPL-3.0-or-later"
}
