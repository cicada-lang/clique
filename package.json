{
  "name": "@cicada-lang/lambda",
  "version": "0.0.34",
  "repository": "github:cicada-lang/lambda",
  "files": [
    "lib"
  ],
  "bin": {
    "lambda": "bin/lambda.js"
  },
  "scripts": {
    "prepare": "husky install",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test:ts": "vitest --dir src --threads false --run",
    "test:watch": "vitest --dir src --threads false",
    "test:coverage": "vitest --dir src --threads false --run --coverage ",
    "test:gui": "vitest --dir src --threads false --ui",
    "test:tests": "test-runner test './bin/lambda.js run' 'docs/tests/**/*.(scm|md)'",
    "test:tests-error": "test-runner snapshot-error './bin/lambda.js' 'docs/tests/**/*.error.(scm|md)'",
    "test": "npm run test:ts && npm run test:tests && npm run test:tests-error",
    "format": "prettier src docs --write"
  },
  "dependencies": {
    "@cicada-lang/sexp": "^0.0.28",
    "@xieyuheng/command-line": "^0.0.7",
    "@xieyuheng/ty": "^0.1.17",
    "axios": "^1.1.2",
    "commonmark": "^0.30.0",
    "crypto-browserify": "^3.12.0",
    "fast-deep-equal": "^3.1.3",
    "node-watch": "^0.7.3",
    "picocolors": "^1.0.0",
    "readdirp": "^3.6.0"
  },
  "devDependencies": {
    "@types/commonmark": "^0.27.5",
    "@types/node": "^18.8.5",
    "@xieyuheng/test-runner": "^0.2.5",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.1.1",
    "pretty-quick": "^3.1.3",
    "typescript": "^4.8.4",
    "@vitest/coverage-c8": "^0.24.3",
    "@vitest/coverage-istanbul": "^0.24.3",
    "@vitest/ui": "^0.24.3",
    "vite": "^3.1.8",
    "vitest": "^0.24.3"
  },
  "license": "GPL-3.0-or-later"
}
